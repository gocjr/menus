{
    "name": "gocjr/menus",
    "description": "Menus plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "version": "1.0",
    "authors": [
        {
            "name": "gocjr",
            "email": "giovaniocjr@gmail.com"
        }
    ],
    "require": {
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Menus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Menus\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
